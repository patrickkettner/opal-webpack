{
  "name": "opal-webpack",
  "version": "1.0.5",
  "description": "Opal Ruby module loader for webpack",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "vendor"
  ],
  "scripts": {
    "build_compiler": "opal --no-exit --no-opal -Ilib -c lib/compiler.rb | uglifyjs > vendor/opal-compiler.js",
    "test": "npm run test_only -- test/unit test/integration",
    "test_only": "PATH=test/support:$PATH OPAL_USE_BUNDLER=false OPAL_LOAD_PATH=./test/fixtures:./test/fixtures/load_path mocha",
    "prepare": "npm run build_compiler",
    "prepublish": "npm run prepare",
    "eslint": "eslint index.js lib test --ignore-pattern lib/fs-cache.js --ignore-pattern test/integration/output --ignore-pattern test/support/tweakedOpalCompiler.js --ignore-pattern test/fixtures/source_maps_nested.js",
    "jshint": "jshint index.js lib/*.js test/**/*.js --exclude test/support/tweakedOpalCompiler.js"
  },
  "author": "CJ Lazell",
  "contributors": [
    {
      "name": "David Chang"
    }
  ],
  "keywords": [
    "webpack",
    "loader",
    "opal",
    "ruby",
    "transpiler",
    "module"
  ],
  "license": "MIT",
  "dependencies": {
    "bundle-loader": "^0.5.4",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.4",
    "loader-utils": "^0.2.11",
    "source-map": "^0.4.4",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "glob": "^5.0.14",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "fs-extra": "^0.26.7",
    "jshint": "^2.9.1",
    "left-pad": "^1.0.1",
    "mocha": "^2.4.5",
    "object-assign": "^4.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "uglifyjs": "^2.4.1",
    "webpack": "^1.12.14"
  }
}

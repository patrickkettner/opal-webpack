{
  "name": "opal-webpack",
  "version": "0.10.0-beta2",
  "description": "Opal Ruby module loader for webpack",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "vendor"
  ],
  "scripts": {
    "build_compiler": "bundle exec opal --no-exit -c lib/compiler.rb | uglifyjs > vendor/opal-compiler.js",
    "test": "OPAL_LOAD_PATH=./test/fixtures:./test/fixtures/load_path mocha test/unit test/integration",
    "prepare": "npm run build_compiler",
    "prepublish": "npm run prepare",
    "eslint": "eslint index.js lib test --ignore-pattern lib/fs-cache.js --ignore-pattern test/integration/output"
  },
  "author": "CJ Lazell",
  "contributors": [
    {
      "name": "David Chang"
    }
  ],
  "keywords": [
    "webpack",
    "loader",
    "opal",
    "ruby",
    "transpiler",
    "module"
  ],
  "license": "MIT",
  "dependencies": {
    "bundle-loader": "^0.5.4",
    "exports-loader": "^0.6.2",
    "glob": "^5.0.14",
    "imports-loader": "^0.6.4",
    "loader-utils": "^0.2.11",
    "source-map": "^0.4.4",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "eslint": "^2.7.0",
    "expect.js": "^0.3.1",
    "fs-extra": "^0.26.7",
    "mocha": "^2.4.5",
    "object-assign": "^4.0.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "uglifyjs": "^2.4.1",
    "webpack": "^1.12.14"
  }
}
